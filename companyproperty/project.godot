; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CompanyProperty"
run/main_scene="res://Assets/Scenes/MenuScreen.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://Assets/Scenes/audio_manager.gd"

[display]

window/size/viewport_width=960
window/size/viewport_height=640
window/size/resizable=false
window/stretch/mode="canvas_items"
window/stretch/aspect="ignore"
window/stretch/scale_mode="integer"

[file_customization]

folder_colors={
"res://Assets/Code/": "orange",
"res://Assets/Images/": "pink",
"res://Assets/Scenes/": "red",
"res://Assets/Scripts/": "purple",
"res://Assets/Sounds/": "blue",
"res://Assets/Sprites/": "green",
"res://Assets/TextFiles/": "yellow"
}

[input]

Debug={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":92,"key_label":0,"unicode":92,"location":0,"echo":false,"script":null)
]
}

[rendering]

textures/canvas_textures/default_texture_filter=0
